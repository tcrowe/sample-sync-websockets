{
  "name": "sample-sync-websockets",
  "version": "0.0.1",
  "description": "Fast multiplayer state synchronization via websockets",
  "private": true,
  "dependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.117",
    "@types/socket.io": "^1.4.38",
    "@types/socket.io-client": "^1.4.32",
    "cors": "^2.8.4",
    "decentraland": "latest",
    "decentraland-api": "latest",
    "decentraland-rpc": "latest",
    "express": "^4.16.4",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "ts-node": "^7.0.1"
  },
  "scripts": {
    "watch": "echo 'watch placeholder'",
    "dev-compile": "decentraland-compiler build.json --watch || true",
    "dev-dcl-start": "dcl start --port 8834 --no-browser --no-watch || true",
    "dev-server": "nodemon -q -L -d 1 -w '*.ts' --ext ts --exec ts-node --project tsconfig-server.json --pretty server.ts || true",
    "dev": "run-p dev-*",
    "start": "ts-node --project tsconfig-server.json --pretty server.ts"
  },
  "now": {
    "type": "npm",
    "engines": {
      "node": "^8.12.0"
    }
  }
}
